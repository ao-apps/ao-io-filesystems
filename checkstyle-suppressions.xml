<!DOCTYPE suppressions PUBLIC
  "-//Checkstyle//DTD SuppressionFilter Configuration 1.2//EN"
  "https://checkstyle.org/dtds/suppressions_1_2.dtd">

<suppressions>

  <!-- TODO: Complete Javadocs -->
  <suppress checks="MissingJavadocMethod" />

  <!-- Consistency with standard "IOException" -->
  <suppress
    checks="AbbreviationAsWordInName"
    files="[/\\]com[/\\]aoapps[/\\]io[/\\]filesystems[/\\]RandomFailFileSystem\.java$"
    message="'RandomFailIOException'"
  />

  <!-- Allow non-standard method names in tests -->
  <suppress
    checks="MethodName"
    files="[/\\]test[/\\]java[/\\]com[/\\]aoapps[/\\]io[/\\]filesystems[/\\]"
  />

</suppressions>
